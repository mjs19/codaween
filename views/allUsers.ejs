<div class="page">
  <% users.forEach(function(user) { %>
   <div class="card" style="margin: 1rem; height: 20rem; width:20rem; display:inline-block; align:vertical-align">
     <a href="/users/<%= user.id%>">
     <img class="card-img-top sized" src="<%=user.userPicture %>" alt="no photo available">
   </a>
     <div class="card-block">
       <h3 class="card-title"><%= user.userFirstName %> <%=user.userLastName %></h3>
     </div>
   </div>


<% }) %>

</div>
