<img src="<%= costume.costumePicture %>" alt="Costume Picture" />
<p>
  Costume Name: <%= costume.costumeName %>
  <br>
  Description: <%= costume.costumeDescription %>
  <br>
  Who's Wearing This?
  <br>
  <% costume.users.forEach(function(user) { %>
    <a href="http://localhost:8080/users/<%= user.id %>"><%= user.userFirstName %> <%= user.userLastName %></a>
  <% }) %>
</p>
<a href="<%= costume.costumeLink %>">Buy it on Amazon</a>

<button id="vote" type="button" name="Vote for Costume">Vote for Costume</button>
<% if (mine) { %>
  <a href="/costumes/<%=costume.id%>/edit"><button id="editCostume" type="button" name="Edit Costume">Edit Costume</button></a>
  <button id="deleteCostume" type="button" name="Delete Costume">Delete Costume</button>

<% } %>
